    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include layout="@layout/activity_title" />

        <me.leefeng.library.view.ItemView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:showRightPic="true"
            app:titleText="姓名"></me.leefeng.library.view.ItemView>

        <me.leefeng.library.view.ItemView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:showRightPic="true"
            app:titleText="手机号"></me.leefeng.library.view.ItemView>

        <me.leefeng.library.view.ItemView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:showRightPic="true"
            app:titleText="学员认证"></me.leefeng.library.view.ItemView>
    </LinearLayout>

</RelativeLayout>
